
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<!-- [Head] start -->
@await Component.InvokeAsync("_AdminLayoutHeadComponent");
<!-- [Head] end -->
<!-- [Body] Start -->

<body data-pc-preset="preset-1" data-pc-direction="ltr" data-pc-theme="light">
  <!-- [ Pre-loader ] start -->
<div class="loader-bg">
  <div class="loader-track">
    <div class="loader-fill"></div>
  </div>
</div>
<!-- [ Pre-loader ] End -->
 <!-- [ Sidebar Menu ] start -->
    @await Component.InvokeAsync("_AdminLayoutSidebarComponent");
<!-- [ Sidebar Menu ] end --> <!-- [ Header Topbar ] start -->
    @await Component.InvokeAsync("_AdminLayoutHeaderComponent");
<!-- [ Header ] end -->



  <!-- [ Main Content ] start -->
  <div class="pc-container">
    <div class="pc-content">
      <!-- [ breadcrumb ] start -->
            @await Component.InvokeAsync("_AdminLayoutBreadCrumpComponent")
      <!-- [ breadcrumb ] end -->
      <!-- [ Main Content ] start -->

      @RenderBody()

    </div>
  </div>
  <!-- [ Main Content ] end -->
    @await Component.InvokeAsync("_AdminLayoutFooterComponent");
    <!--  script start  -->
    @await Component.InvokeAsync("_AdminLayoutScriptComponent");
    <!--Script end -->
</body>
<!-- [Body] end -->

</html>